{
    "version": 2,
    "builds": [
      {
        "src": "path/to/your/source",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "path/to/your/api-file.js"
      },
      {
        "src": "/(.*)",
        "dest": "path/to/your/static-folder/$1"
      }
    ]
  }
  